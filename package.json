{
  "name": "CurrencyField",
  "sencha": {
    "namespace": "CurrencyField",
    "type": "code",
    "creator": "Levan Tskipuri",
    "summary": "An extension of numberfield to reflect choice of the currency",
    "version": "1.0.0",
    "compatVersion": "1.0.0",
    "format": "1",
    "slicer": {
      "js": [
        {
          "path": "${package.dir}/sass/example/custom.js",
          "isWidgetManifest": true
        }
      ]
    },
    "output": "${package.dir}/build",
    "local": true,
    "sass": {
      "namespace": "CurrencyField",
      "etc": ["${package.dir}/sass/etc/all.scss"],
      "var": ["${package.dir}/sass/var"],
      "src": ["${package.dir}/sass/src"]
    },
    "classpath": ["${package.dir}/src"],
    "overrides": ["${package.dir}/overrides"],
    "example": {
      "path": ["${package.dir}/examples"]
    },
    "requires": []
  }
}
